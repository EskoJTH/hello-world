import Data.Time.Clock
import Data.Map as M
data Vesa = Vesa {project:: String, subproject :: String, starttime :: UTCTime, endtime :: UTCTime, comment :: String} deriving (Eq,Show)




newtype MonMap k a = Mon (M.Map k a) deriving (Show)

instance (Ord k, Monoid m) => Monoid (MonMap k m) where
    mempty = Mon (M.empty)
    mappend (Mon m1) (Mon m2) = Mon (M.unionWith mappend m1 m2)